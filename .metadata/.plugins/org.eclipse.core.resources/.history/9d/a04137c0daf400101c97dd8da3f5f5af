package exam;

public class Q2884 {

	public static void main(String[] args) {
		int hour = 10;
		int minute = 10;
		
		if(minute >= 45) {
			minute -= 45;
		} else if(hour > 0){
			hour--;
			minute = minute + 60 - 45;
		} else {
			hour = 23;
			minute = minute + 60 - 45;
		}
		System.out.println(hour + " " + minute);

		hour = 0;
		minute = 30;
		
		if(minute >= 45) {
			minute -= 45;
		} else if(hour > 0){
			hour--;
			minute = minute + 60 - 45;
		} else {
			hour = 23;
			minute = minute + 60 - 45;
		}
		System.out.println(hour + " " + minute);

		hour = 23;
		minute = 40;
		
		if(minute >= 45) {
			minute -= 45;
		} else if(hour > 0){
			hour--;
			minute = minute + 60 - 45;
		} else {
			hour = 23;
			minute = minute + 60 - 45;
		}
		System.out.println(hour + " " + minute);
		
		
		

	}

}
