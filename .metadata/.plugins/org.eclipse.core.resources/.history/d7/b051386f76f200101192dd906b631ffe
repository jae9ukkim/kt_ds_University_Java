package exam;

public class ArithmaticQuiz4 {

	public static void main(String[] args) {
		int subjectA = 100;
		int subjectB = 85;
		int subjectC = 95;
		int subjectD = 68;
		
		int totalScore = 0; // 모든 과목들의 합
		double average = 0; // 평균점수
		double score = 0; // 4.5 만점 기준 학점 계산. 숫점 이하 두자리까지
		
		System.out.println("점수 총합: " + totalScore);
		System.out.println("점수 평균: " + average);
		System.out.println("학점 결과: " + score);
	}

}
