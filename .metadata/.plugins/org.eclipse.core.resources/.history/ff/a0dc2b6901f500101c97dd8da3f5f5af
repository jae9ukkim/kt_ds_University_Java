package exam;

public class ForQuiz3 {

	public static void main(String[] args) {
		for(int i = 1; i < 101; i++) {
			if(i % 3 == 0) {
				System.out.println(i);
			}
		}
		
		for(int i = 1; i < 101; i++) {
			if(i % 5 == 0) {
				System.out.println(i);
			}
		}
		
		for(int i = 1; i < 101; i++) {
			if(i % 6 == 0) {
				System.out.println(i);
			}
		}
		
		for(int i = 1; i < 101; i++) {
			if(i % 3 == 0 || i % 5 == 0 || i % 6 == 0) {
				System.out.println(i);
			}
		}
		
		

	}

}
