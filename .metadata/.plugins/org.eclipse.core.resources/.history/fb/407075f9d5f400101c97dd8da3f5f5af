package exam;

public class Q2480 {

	public static void main(String[] args) {
		int dice[] = { 3, 3, 6 };
		int prize = 0;
		
		if (dice[0] == dice[1] && dice[0] == dice[2]) {
			prize += 10000;
			prize += dice[0] * 1000;
		} else if(dice[0] == dice[1] || dice[0] == dice[2] || dice[1] == dice[2] ) {
			prize += 1000;
			prize += dice[0] * 100;
		}

	}

}
